{
	"name": "CopyOrdersFromAwsFemacorp",
	"properties": {
		"description": "CopyPipeline_AwsToAzureBlobStore",
		"activities": [
			{
				"name": "Copy_fmg",
				"type": "Copy",
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [
					{
						"name": "Source",
						"value": "*/"
					},
					{
						"name": "Destination",
						"value": "femacorp/"
					}
				],
				"typeProperties": {
					"source": {
						"type": "FileSystemSource",
						"recursive": true,
						"maxConcurrentConnections": 10
					},
					"sink": {
						"type": "BlobSink",
						"copyBehavior": "PreserveHierarchy"
					},
					"enableStaging": false
				},
				"inputs": [
					{
						"referenceName": "AmazonS3Object1_Femacorp",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "AzureBlobStoreFemacorpOrders",
						"type": "DatasetReference"
					}
				]
			},
			{
				"name": "RemoveFileAfterCopy",
				"description": "Remove the source file after it has been copied",
				"type": "Delete",
				"dependsOn": [
					{
						"activity": "Copy_fmg",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"typeProperties": {
					"dataset": {
						"referenceName": "AmazonS3Object1_Femacorp",
						"type": "DatasetReference"
					},
					"enableLogging": false
				}
			}
		]
	},
	"type": "Microsoft.DataFactory/factories/pipelines"
}